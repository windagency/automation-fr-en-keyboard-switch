<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE plist PUBLIC "-//Apple//DTD PLIST 1.0//EN" "http://www.apple.com/DTDs/PropertyList-1.0.dtd">
<plist version="1.0">
  <dict>
    <key>Label</key>
    <string>com.wind.keyboardwatcher</string>

    <key>ProgramArguments</key>
    <array>
      <string>/usr/bin/osascript</string>
      <string>/Users/damientivelet/Library/Scripts/com.wind.keyboardwatcher.applescript</string>
    </array>

    <key>RunAtLoad</key>
    <true />

    <key>KeepAlive</key>
    <dict>
      <key>SuccessfulExit</key>
      <true />
      <key>OtherExit</key>
      <true />
    </dict>

    <key>StartInterval</key>
    <integer>300</integer>

    <key>ThrottleInterval</key>
    <integer>10</integer>

    <key>WatchPaths</key>
    <array>
      <string>/usr/sbin/ioreg</string>
      <string>/Users/damientivelet/Library/Scripts/log/com.wind.keyboardwatcher.stdout.log</string>
    </array>

    <key>StandardOutPath</key>
    <string>/Users/damientivelet/Library/Scripts/log/com.wind.keyboardwatcher.stdout.log</string>
    <key>StandardErrorPath</key>
    <string>/Users/damientivelet/Library/Scripts/log/com.wind.keyboardwatcher.stderr.log</string>
  </dict>
</plist>